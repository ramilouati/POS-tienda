{
    "version": 2,

    "builds": [
        {
            "src": "store/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "store/wsgi.py"
        }
    ]
}